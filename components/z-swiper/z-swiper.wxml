<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper"
        indicator-dots='true'
        indicator-active-color='#d81e06'
        autoplay='true'
        circular='true'
        interval='3000'> 
		 <block wx:for="{{list}}" wx:key="index">
			<swiper-item class='swiper-item'>
				<image src="{{item.img }}" mode='widthFix'/>
			</swiper-item>
		</block>
</swiper>
